<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="rettalike" _name="Rettalike" version="1.0">
	<author>Muhammed Uluyol</author>
	<_description>Based on &quot;Retta&quot; Eclipse Color Theme By Eric</_description>

<!-- Scribe Theme Generator 1.3 -->

<!-- COLORS -->
	<color name="comment_color" value="#83786e"/>
	<color name="number_color" value="#d6c248"/>
	<color name="string_color" value="#d6c248"/>
	<color name="boolean_color" value="#527d5d"/>
	<color name="keyword_color" value="#e79e3c"/>
	<color name="error_color" value="#ff0000"/>
	<color name="text_fg" value="#f8e1aa"/>
	<color name="text_bg" value="#000000"/>
	<color name="line_highlight" value="#2a2a2a"/>

<!-- Global Settings -->
	<style name="text" foreground="text_fg" background="text_bg"/>
	<style name="cursor" foreground="text_fg"/>
	<style name="current-line" background="line_highlight"/>
	<style name="line-numbers" foreground="text_fg" background="line_highlight"/>
	<style name="selection" foreground="text_fg" background="#527d5d"/>

<!-- Bracket Matching -->
	<style name="bracket-match" foreground="text_bg" background="keyword_color"/>
	<style name="bracket-mismatch" background="error_color"/>
	<style name="search-match" foreground="text_fg" background="#395EB1"/>

<!-- Base syntax colors

Languages: plain, python, ruby, xml, html, ada, asp, awk, boo, c,
changelog, chdr, cmake, cpp, csharp, css, d, def, desktop, diff,
docbook, dosbatch, dot, dpatch, dtd, eiffel, erlang, forth, fortran,
gap, gtkrc, haddock, haskell, haskell-literate, idl, ini, java,
javascript, latex, libtool, lua, m4, makefile, msil, nermele, nsis,
objc, ocaml, ocl, octave, pascal, perl, php, pkgconfig, po, prolog,
R, rpmspec, scheme, sh, sql, t2t, tcl, texinfo, vala, vbnet, verilog,
vhdl, xslt, yacc

-->

	<style name="def:string" foreground="string_color"/>
	<style name="def:character" foreground="string_color" bold="true"/>
	<style name="def:special-char" foreground="#BFA4A4"/>
	<style name="def:number" foreground="number_color"/>
	<style name="def:decimal" foreground="number_color"/>
	<style name="def:floating-point" foreground="number_color"/>
	<style name="def:complex" foreground="number_color"/>
	<style name="def:base-n-integer" foreground="number_color"/>
	<style name="def:boolean" foreground="keyword_color" bold="true"/>

	<style name="def:builtin" foreground="keyword_color" bold="true"/>
	<style name="def:keyword" foreground="keyword_color" bold="true"/>
	<style name="def:function" foreground="keyword_color" bold="true"/>
	<style name="def:type" foreground="#DE6546" bold="true"/>
	<style name="def:signals" foreground="keyword_color" bold="true"/>
	<style name="def:preprocessor" foreground="keyword_color" bold="true"/>
	<style name="def:identifier" foreground="text_fg"/>
	<style name="def:operator" foreground="number_color"/>
	<style name="def:statement" foreground="keyword_color" bold="true"/>

	<style name="def:constant" foreground="#EFC090"/>
	<style name="def:special-constant" foreground="keyword_color" bold="true"/>
	<style name="def:error" foreground="text_bg" background="#DE6546"/>

	<style name="xml:attribute-name" foreground="#DE6546"/>
	<style name="xml:doctype" foreground="#A4B0C0"/>
	<style name="c:signal-name" foreground="keyword_color" bold="true"/>
	<style name="c:standard-stream" foreground="#EFC090" bold="true"/>
	<style name="java:scope-declaration" foreground="keyword_color" bold="true"/>
	<style name="java:declaration" foreground="keyword_color" bold="true"/>

<!-- Comments -->

	<style name="def:comment" foreground="comment_color"/>
	<style name="def:shebang" foreground="comment_color"/>
	<style name="def:doc-comment-element" foreground="#83786E"/>
	<style name="def:note" foreground="#83786E"/>

</style-scheme>
